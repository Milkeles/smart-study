{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Smart Study",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/bin/smart-study",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "preLaunchTask": "CMake Build",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "miDebuggerPath": "/usr/bin/gdb"
    }
  ]
}